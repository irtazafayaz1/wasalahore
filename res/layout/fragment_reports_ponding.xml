<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ponding.ReportsPondingFragment">


    <com.evrencoskun.tableview.TableView
        android:id="@+id/content_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        app:column_header_height="@dimen/default_column_header_height"
        app:row_header_width="@dimen/default_row_header_width"
        app:selected_color="@android:color/holo_blue_dark"
        app:shadow_color="@android:color/holo_orange_dark"
        app:unselected_color="@android:color/holo_red_light" />

</FrameLayout>